{
  "title": "{{.Title}}",
  {{if .DashId}}"id": {{.DashId}},{{end}}
  "tags": {{.Tags}},
  "style": "dark",
  "timezone": "browser",
  "editable": false,
  "hideControls": false,
  "sharedCrosshair": false,
  "rows": [
    {
      "title": "Locations",
      "collapse": false,
      "editable": false,
      "height": "250px",
      "panels": [
{{range .Panels}}
        {
          "aliasColors": {
            "up": "#7EB26D",
            "down": "#BF1B00"
          },
          "annotate": {
            "enable": false
          },
          "bars": false,
          "datasource": "raintank",
          "editable": false,
          "error": false,
          "fill": 0,
          "grid": {
            "leftMax": null,
            "leftMin": null,
            "rightMax": 1,
            "rightMin": 0,
            "threshold1": null,
            "threshold1Color": "rgba(216, 200, 27, 0.27)",
            "threshold2": null,
            "threshold2Color": "rgba(234, 112, 112, 0.22)"
          },
          "id": {{.Id}},
          "legend": {
            "avg": false,
            "current": false,
            "max": false,
            "min": false,
            "show": true,
            "total": false,
            "values": false
          },
          "lines": true,
          "linewidth": 1,
          "nullPointMode": "null",
          "percentage": false,
          "pointradius": 5,
          "points": false,
          "renderer": "flot",
          "resolution": 100,
          "scale": 1,
          "seriesOverrides": [
             {
              "alias": "up",
              "fill": 1,
              "linewidth": 0,
              "steppedLine": true,
              "yaxis": 2,
              "zindex": -3
            },
            {
              "alias": "down",
              "fill": 1,
              "linewidth": 0,
              "steppedLine": true,
              "yaxis": 2,
              "zindex": -3
            }
          ],
          "span": 6,
          "stack": true,
          "steppedLine": false,
          "targets": [
            {
              "target": "aliasByNode({{.Namespace}}.http.{{.Slug}}.{{.Location}}.{dns,connect,send,wait,recv}, 4)"
            },
            {
              "target": "alias({{.Namespace}}.http.{{.Slug}}.{{.Location}}.state, 'down')"
            },
            {
              "target": "alias(scale(offset({{.Namespace}}.http.{{.Slug}}.{{.Location}}.state, -1), -1), 'up')"
            }
          ],
          "timeFrom": null,
          "timeShift": null,
          "title": "{{.Title}}",
          "tooltip": {
            "query_as_alias": true,
            "shared": true,
            "value_type": "individual"
          },
          "type": "graph",
          "x-axis": true,
          "y-axis": true,
          "y_formats": [
            "ms",
            "short"
          ],
          "zerofill": true
        }{{if not .Last}},{{end}}
{{end}}
      ]
    }
  ],
  "nav": [
    {
      "collapse": false,
      "enable": true,
      "notice": false,
      "now": true,
      "refresh_intervals": [
        "5s",
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ],
      "status": "Stable",
      "time_options": [
        "5m",
        "15m",
        "1h",
        "6h",
        "12h",
        "24h",
        "2d",
        "7d",
        "30d"
      ],
      "type": "timepicker"
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "templating": {
    "list": []
  },
  "annotations": {
    "list": []
  },
  "refresh": false,
  "version": 6,
  "hideAllLegends": false
}
